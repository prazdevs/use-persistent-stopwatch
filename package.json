{
  "name": "use-persistent-stopwatch",
  "version": "1.2.0",
  "description": "Persistent stopwatch Vue composable",
  "author": "Sacha 'PraZ' Bouillez (https://praz.dev)",
  "repository": "github:prazdevs/use-persistent-stopwatch",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && rollup -c",
    "size": "size-limit",
    "test": "yarn test:vue3 && yarn test:vue2",
    "test:vue2": "vue-demi-switch 2 vue2 && jest",
    "test:vue3": "vue-demi-switch 3 && jest"
  },
  "size-limit": [
    {
      "path": "dist/index.esm.js",
      "limit": "5 kB"
    }
  ],
  "main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vueuse/core": "^6.0.0",
    "vue-demi": "^0.11.3"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.1.3",
    "vue": "^2.6.14"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/jest": "^27.0.1",
    "@vue/composition-api": "^1.1.3",
    "codecov": "^3.8.3",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "size-limit": "^5.0.3",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5",
    "vue": "^3.2.4",
    "vue2": "npm:vue@^2.6.14"
  }
}
