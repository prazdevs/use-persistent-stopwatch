{
  "name": "use-persistent-stopwatch",
  "version": "1.1.1",
  "description": "Persistent stopwatch Vue composable",
  "author": "Sacha 'PraZ' Bouillez (https://praz.dev)",
  "repository": "github:prazdevs/use-persistent-stopwatch",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && rollup -c",
    "size": "size-limit",
    "test": "yarn test:vue3 && yarn test:vue2",
    "test:vue2": "vue-demi-switch 2 vue2 && jest",
    "test:vue3": "vue-demi-switch 3 && jest"
  },
  "size-limit": [
    {
      "path": "dist/index.esm.js",
      "limit": "5 kB"
    }
  ],
  "main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vueuse/core": "^5.0.2",
    "vue-demi": "^0.9.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.11",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/jest": "^26.0.23",
    "@vue/composition-api": "^1.0.0-rc.9",
    "codecov": "^3.8.2",
    "jest": "^27.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.1",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "size-limit": "^4.12.0",
    "ts-jest": "^27.0.3",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2",
    "vue": "^3.0.5",
    "vue2": "npm:vue@^2.6.14"
  }
}
