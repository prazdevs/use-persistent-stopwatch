{
  "name": "use-persistent-stopwatch",
  "version": "1.3.0",
  "description": "Persistent stopwatch Vue composable",
  "author": "Sacha 'PraZ' Bouillez (https://praz.dev)",
  "repository": "github:prazdevs/use-persistent-stopwatch",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && rollup -c",
    "size": "size-limit",
    "test": "yarn test:vue3 && yarn test:vue2",
    "test:vue2": "vue-demi-switch 2 vue2 && jest",
    "test:vue3": "vue-demi-switch 3 && jest"
  },
  "size-limit": [
    {
      "path": "dist/index.esm.js",
      "limit": "5 kB"
    }
  ],
  "main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vueuse/core": "^9.5.0",
    "vue-demi": "^0.13.11"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^27.5.2",
    "@vue/composition-api": "^1.7.1",
    "codecov": "^3.8.3",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.5",
    "typescript": "^4.8.4",
    "vue": "^3.2.45",
    "vue2": "npm:vue@^2.6.14"
  }
}
